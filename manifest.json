{
    "name": "FormFillinExtension",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["declarativeContent", "activeTab"],
    "background": {
        "scripts": ["jquery-2.1.3.min.js", "background.js"],
        "persistent": false
      },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["jquery-2.1.3.min.js", "content.js"]
        }
    ],
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": "images/icon.png"
    }
}