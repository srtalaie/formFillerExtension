{
    "name": "formFillerExtension",
    "manifest_version": 2,
    "version": "1.0",
    "permissions": ["tabs", "*://*.walmart.com/checkout/*/payment"],
    "content_scripts": [
        {
            "matches": ["*://*.walmart.com/checkout/*/payment"],
            "js": ["content.js"]
        }
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "/images/icon.png",
        "default_popup": "popup.html",
        "default_title": "A popup will come here."
    }
}