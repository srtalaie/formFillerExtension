{
    "name": "FormFillinExtension",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["jquery-2.1.3.min.js", "content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "images/icon.png"
    }
}